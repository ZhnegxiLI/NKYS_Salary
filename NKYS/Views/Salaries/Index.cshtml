            <th>
                @Html.DisplayNameFor(model => model.SocialSercurityFee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HousingReserves)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FullPresencePay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OvertimePay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AbsentDeduct)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DormFee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransportFee)
